{
    "name": "obidev-lambda-deployer",
    "displayName": "AWS Lambda Deployer",
    "description": "AWS Lambda fonksiyonlarını VS Code'dan kolayca deploy edin",
    "version": "0.0.1",
    "icon": "icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/huseyinkaracif/obidev-lambda-deployer.git"
    },
    "publisher": "huseyinkaracif",
    "engines": {
        "vscode": "^1.74.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "aws",
        "lambda",
        "deploy",
        "serverless"
    ],
    "files": [
        "out/extension.js",
        "README.md",
        "LICENSE",
        "icon.png"
    ],
    "activationEvents": [
        "onCommand:obidev-lambda-deployer.deployLambda"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "obidev-lambda-deployer.deployLambda",
                "title": "AWS Deploy Et"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "obidev-lambda-deployer.deployLambda",
                    "group": "navigation",
                    "when": "resourceExtname == .js || resourceExtname == .mjs || resourceExtname == .ts || resourceExtname == .py || resourceExtname == .java"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile && npm run bundle",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "bundle": "webpack --mode production",
        "package": "vsce package",
        "publish": "vsce publish"
    },
    "devDependencies": {
        "@types/vscode": "^1.74.0",
        "@types/node": "16.x",
        "typescript": "^4.9.4",
        "webpack": "^5.88.0",
        "webpack-cli": "^5.1.0",
        "ts-loader": "^9.4.0",
        "vsce": "^2.15.0"
    },
    "dependencies": {
        "jszip": "^3.10.1"
    }
}