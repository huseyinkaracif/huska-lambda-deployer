{
    "name": "obidev-lambda-deployer",
    "displayName": "AWS Lambda Deployer",
    "description": "AWS Lambda fonksiyonlarını VS Code'dan kolayca deploy edin",
    "version": "0.0.1",
    "repository": {
        "type": "git",
        "url": "https://github.com/huseyinkaracif/obidev-lambda-deployer.git"
    },
    "publisher": "huseyinkaracif",
    "engines": {
        "vscode": "^1.74.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "aws",
        "lambda",
        "deploy",
        "serverless"
    ],
    "activationEvents": [
        "onCommand:obidev-lambda-deployer.deployLambda"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "obidev-lambda-deployer.deployLambda",
                "title": "AWS Deploy Et"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "obidev-lambda-deployer.deployLambda",
                    "group": "navigation",
                    "when": "resourceExtname == .js || resourceExtname == .mjs || resourceExtname == .ts || resourceExtname == .py || resourceExtname == .java"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/vscode": "^1.74.0",
        "@types/node": "16.x",
        "typescript": "^4.9.4"
    },
    "dependencies": {
        "aws-sdk": "^2.1400.0",
        "jszip": "^3.10.1"
    }
}